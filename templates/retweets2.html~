<script>
function runQuery(query, handler) {  
    gadgets.io.makeRequest(BASE_URI, handler, {  
        METHOD: 'POST',  
        POST_DATA: toQueryString({q: query, format: 'json'}),  
        CONTENT_TYPE: 'JSON',  
        AUTHORIZATION: 'OAuth'  
    });  
  };
</script>










